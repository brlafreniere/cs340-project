<%- contentFor('header') %>
Pilots

<%- contentFor('content') %>

<div class="mt-3">
  <a href="/pilots/new">New Pilot</a>
</div>

<table class="table table-striped border mt-3">
  <thead class="thead-dark">
    <tr>
      <th scope="col">Pilot ID</th>
      <th scope="col">First Name</th>
      <th scope="col">Last Name</th>
      <th scope="col">Edit</th>
      <th scope="col">Delete</th>
    </tr>
  </thead>

  <tbody class="table-group-divider">
    <% for (const pilot of pilots) { %>
      <tr>
        <td><%= pilot.pilot_id %></td>
        <td><%= pilot.first_name %></td>
        <td><%= pilot.last_name %></td>
        <td><a href="/pilots/<%= pilot.pilot_id %>/edit">edit</a></td>
        <td><a href="/pilots/<%= pilot.pilot_id %>/confirm_delete">delete</a></td>
      </tr>
    <% } %>
  </tbody>
</table>



  </body>
</html>
